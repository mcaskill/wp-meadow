{
    "type": "library",
    "name": "mcaskill/wp-meadow",
    "description": "WordPress templating with Twig",
    "keywords": [ "wordpress", "twig" ],
    "homepage": "https://github.com/mcaskill/wp-meadow",
    "license": "MIT",
    "authors": [
        {
            "name": "Chauncey McAskill",
            "homepage": "https://mcaskill.ca/"
        },
        {
            "name": "Andrey Savchenko",
            "homepage": "https://www.Rarst.net/"
        }
    ],
    "support": {
        "issues": "https://github.com/mcaskill/wp-meadow/issues"
    },
    "require": {
        "php": ">=7.0",
        "pimple/pimple": "^3.2.3",
        "twig/twig": "^2.6"
    },
    "require-dev": {
        "automattic/phpcs-neutron-ruleset": "^3.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "McAskill\\Meadow\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
